	<AppSet Key="DefaultBusinessRoute" Value="DefaultRoute" ToClient="false" Description="默认的业务路由设置，在扩展名为.businessroute配置的文件中的名称，如果未设置或者不存在取默认的路由,可选值LoadBalanceRoute、DefaultRoute" />
	<AppSet Key="BusinessRouteListenPort" Value="9000" ToClient="false" Description="使用负载均衡路由时 监听的端口号，通过该端口号进行服务发现" />
	<AppSet Key="BusinessRouteMaxTimespan" Value="30" ToClient="false" Description="使用负载均衡路由时 最大的时间间隔 当大于该值时，代表已经离线，需要从地址列表中移除，单位秒" />
	<AppSet Key="BusinessRoutePollTimespan" Value="5000" ToClient="false" Description="使用负载均衡路由时 多长时间轮询一次，单位毫秒" />
	<AppSet Key="LoadBalanceHandleType" Value="DivideEqual" ToClient="false" Description="使用负载均衡路由提供方式，可选值：ConsistentHashing（一致哈希）、DivideEqual（请求平分）" />
	<AppSet Key="EnableCodeValueShowSmartMode" Value="true" ToClient="false" Description="码值加载精简模式" />

	<AppSet Key="MailWindowCloseInterval" Value="20" ToClient="true" Description="内部邮件提示框的停留时间(秒)" />
	<AppSet Key="UploadFileExtend" Value="txt,dwg,doc,docx,xls,xlsx,ppt,pptx,pdf,svl,gif,png,jpg,jpeg,bmp,svg,dwf,rar,zip,7z,avi,mp4,rm,rmvb,mkv,exe,wmv,dll,xml,mpp,log,flv" ToClient="true" Description="上传文件后缀名扩展" />
</root>