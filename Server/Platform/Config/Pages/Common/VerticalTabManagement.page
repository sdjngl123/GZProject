<?xml version="1.0" encoding="utf-8" ?>
<root Name="VerticalTabManagement" IsCache="true">
  <Settings Key="height" Value="600"/>
  <Settings Key="width" Value="800"/>
  <PageHandlers>
    <PageHandler HandlerName ="OnCreate" Script="InforCenter_Platform_VerticalTabManagement_LoadPage(pageEvent);"/>
  </PageHandlers>
  <PageControls>
    <!--一层-->
    <PageControl ControlID="VerticalTabManagement_Container" ParentID="" TypeName="LayoutContainer">
      <Settings Key="item1">
        <Settings Key="size" Value="100%"/>
        <Settings Key="childid" Value="VerticalTabsCtrlPage"/>
      </Settings>
    </PageControl>
    <!--四层-->
    <PageControl ControlID="VerticalTabsCtrlPage" ParentID="VerticalTabManagement_Container" TypeName="PageContainer">
      <Settings Key="pageName" Value="VerticalTabsCtrl"/>
    </PageControl>
  </PageControls>
</root>