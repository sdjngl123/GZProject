<?xml version="1.0" encoding="utf-8" ?>
<root Name="SetLocalFileServiceAddressPage">
  <Settings Key="title" Value="SetLocalFileServiceAddress.Title"/>
  <Settings Key="height" Value="240"/>
  <Settings Key="width" Value="370"/>
  <PageHandlers>
    <PageHandler HandlerName ="OnCreate" Script="InforCenter_Registered_SetLocalFileServiceAddress_OnCreate(pageEvent);"/>
  </PageHandlers>
  <PageControls>
    <PageControl ControlID="Top_Container" ParentID="" TypeName="LayoutContainer">
      <Settings Key="type" Value="vertical"/>
      <Settings Key="item0">
        <Settings Key="size" Value="10"/>
      </Settings>
      <Settings Key="item1">
        <Settings Key="size" Value="100%"/>
        <Settings Key="childid" Value="Info_Container"/>
      </Settings>
      <Settings Key="item2">
        <Settings Key="size" Value="1"/>
        <Settings Key="type" Value="drag"/>
        <Settings Key="resizable" Value="false"/>
        <Settings Key="closable" Value="false"/>
      </Settings>
      <Settings Key="item3">
        <Settings Key="size" Value="60"/>
        <Settings Key="childid" Value="Btn_Container"/>
      </Settings>
    </PageControl>
    <PageControl ControlID="Info_Container" ParentID="Top_Container" TypeName="LayoutContainer">
      <Settings Key="type" Value="vertical"/>
      <Settings Key="item1" Value="">
        <Settings Key="size" Value="43"/>
        <Settings Key="childid" Value="IP"/>
      </Settings>
      <Settings Key="item2" Value="">
        <Settings Key="size" Value="43"/>
        <Settings Key="childid" Value="Port"/>
      </Settings>
      <Settings Key="item3" Value="">
        <Settings Key="size" Value="43"/>
        <Settings Key="childid" Value="Volumn"/>
      </Settings>
    </PageControl>
    <PageControl ControlID="IP" ParentID="Info_Container" TypeName="LayoutContainer">
      <Settings Key="type" Value="horizontal"/>
      <Settings Key="item2" Value="">
        <Settings Key="size" Value=""/>
        <Settings Key="childid" Value="IP_Label"/>
      </Settings>
      <Settings Key="item3" Value="">
        <Settings Key="size" Value="50%"/>
        <Settings Key="childid" Value="IP_Value"/>
      </Settings>
      <Settings Key="item1" Value="">
        <Settings Key="size" Value="20"/>
        <Settings Key="childid" Value="IP_Label_FieldFlag"/>
      </Settings>
    </PageControl>
    <PageControl ControlID="IP_Label" ParentID="IP" TypeName="Label" >
      <Settings Key="position" Value="right"/>
      <Settings Key="textId" Value="SetLocalFileServiceAddress.IP" />
    </PageControl>
    <PageControl ControlID="IP_Label_FieldFlag" ParentID="IP" TypeName="Label" >
      <Settings Key="position" Value="left"/>
      <Settings Key="textId" Value="Platform.RequiredFlag" />
      <Settings Key="color" Value="red" />
    </PageControl>
    <PageControl ControlID="IP_Value" ParentID="IP" TypeName="TextBox" >
      <Settings Key="type" Value="text"/>
      <Settings Key="regex" Value="^\s*(\S+\s*)+$ AND ^([\s\S]{0,100})?$" />
      <Settings Key="regextipId" Value="SetLocalFileServiceAddress.IPAddressRegex" />
    </PageControl>

    <PageControl ControlID="Port" ParentID="Info_Container" TypeName="LayoutContainer">
      <Settings Key="type" Value="horizontal"/>
      <Settings Key="item2" Value="">
        <Settings Key="size" Value=""/>
        <Settings Key="childid" Value="Port_Label"/>
      </Settings>
      <Settings Key="item3" Value="">
        <Settings Key="size" Value="50%"/>
        <Settings Key="childid" Value="Port_Value"/>
      </Settings>
      <Settings Key="item1" Value="">
        <Settings Key="size" Value="20"/>
        <Settings Key="childid" Value="Port_Label_FieldFlag"/>
      </Settings>
    </PageControl>
    <PageControl ControlID="Port_Label" ParentID="Port" TypeName="Label" >
      <Settings Key="position" Value="right"/>
      <Settings Key="textId" Value="SetLocalFileServiceAddress.Port" />
    </PageControl>
    <PageControl ControlID="Port_Label_FieldFlag" ParentID="Port" TypeName="Label" >
      <Settings Key="position" Value="left"/>
      <Settings Key="textId" Value="Platform.RequiredFlag" />
      <Settings Key="color" Value="red" />
    </PageControl>
    <PageControl ControlID="Port_Value" ParentID="Port" TypeName="TextBox" >
      <Settings Key="type" Value="text"/>
      <Settings Key="regex" Value="^\s*(\S+\s*)+$ AND ^([\s\S]{0,100})?$" />
      <Settings Key="regextipId" Value="SetLocalFileServiceAddress.PortRegex" />
    </PageControl>

    <PageControl ControlID="Volumn" ParentID="Info_Container" TypeName="LayoutContainer">
      <Settings Key="type" Value="horizontal"/>
      <Settings Key="item2" Value="">
        <Settings Key="size" Value=""/>
        <Settings Key="childid" Value="Volumn_Label"/>
      </Settings>
      <Settings Key="item3" Value="">
        <Settings Key="size" Value="50%"/>
        <Settings Key="childid" Value="Volumn_Value"/>
      </Settings>
      <Settings Key="item1" Value="">
        <Settings Key="size" Value="20"/>
        <Settings Key="childid" Value="Volumn_Label_FieldFlag"/>
      </Settings>
    </PageControl>
    <PageControl ControlID="Volumn_Label" ParentID="Volumn" TypeName="Label" >
      <Settings Key="position" Value="right"/>
      <Settings Key="textId" Value="SetLocalFileServiceAddress.Volumn" />
    </PageControl>
    <PageControl ControlID="Volumn_Label_FieldFlag" ParentID="Port" TypeName="Label" >
      <Settings Key="position" Value="left"/>
      <Settings Key="textId" Value="Platform.RequiredFlag" />
      <Settings Key="color" Value="red" />
    </PageControl>
    <PageControl ControlID="Volumn_Value" ParentID="Volumn" TypeName="TextBox" >
      <Settings Key="type" Value="text"/>
      <Settings Key="regex" Value="^\s*(\S+\s*)+$ AND ^([\s\S]{0,100})?$" />
      <Settings Key="regextipId" Value="SetLocalFileServiceAddress.VolumnRegex" />
    </PageControl>

    <PageControl ControlID="Btn_Container" ParentID="Top_Container" TypeName="LayoutContainer">
      <Settings Key="type" Value="horizontal"/>
      <Settings Key="item1" Value="">
        <Settings Key="size" Value="42%"/>
        <Settings Key="childid" Value=""/>
      </Settings>
      <Settings Key="item2" Value="">
        <Settings Key="size" Value="90"/>
        <Settings Key="childid" Value="btnOK"/>
      </Settings>
      <Settings Key="item3" Value="">
        <Settings Key="size" Value="16%"/>
        <Settings Key="childid" Value=""/>
      </Settings>
      <Settings Key="item4" Value="">
        <Settings Key="size" Value="90"/>
        <Settings Key="childid" Value="btnCancel"/>
      </Settings>
      <Settings Key="item5" Value="">
        <Settings Key="size" Value="42%"/>
        <Settings Key="childid" Value=""/>
      </Settings>
    </PageControl>
    <PageControl ControlID="btnOK" ParentID="Btn_Container" TypeName="Button" >
      <PageHandlers>
        <PageHandler HandlerName ="OnClick" Script="InforCenter_Registered_SetLocalFileServiceAddress_OnOK(ctrlEvent);"/>
      </PageHandlers>
      <Settings Key="textId" Value="Common.OK" />
    </PageControl>
    <PageControl ControlID="btnCancel" ParentID="Btn_Container" TypeName="Button" >
      <PageHandlers>
        <PageHandler HandlerName ="OnClick" Script="HoteamUI.UIManager.Return(ctrlEvent.o.ContainerID(), null);"/>
      </PageHandlers>
      <Settings Key="textId" Value="Common.Cancel" />
    </PageControl>
  </PageControls>
</root>